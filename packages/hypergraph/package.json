{
  "name": "@graphprotocol/hypergraph",
  "version": "0.0.1",
  "description": "SDK for building performant, type-safe, local-first dapps on top of The Graph ecosystem knowledge graphs.",
  "repository": {
    "type": "git",
    "url": "https://github.com/geobrowser/graph-framework.git",
    "directory": "packages/hypergraph"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "default": "./build/esm/index.js",
      "types": "./build/dts/index.d.ts"
    },
    "./utils": {
      "default": "./build/esm/utils/index.js",
      "types": "./build/dts/utils/index.d.ts"
    },
    "./key": {
      "default": "./build/esm/key/index.js",
      "types": "./build/dts/key/index.d.ts"
    },
    "./schema": {
      "default": "./build/esm/schema/index.js",
      "types": "./build/dts/schema/index.d.ts"
    },
    "./space-events": {
      "default": "./build/esm/space-events/index.js",
      "types": "./build/dts/space-events/index.d.ts"
    },
    "./messages": {
      "default": "./build/esm/messages/index.js",
      "types": "./build/dts/messages/index.d.ts"
    },
    "./identity": {
      "default": "./build/esm/identity/index.js",
      "types": "./build/dts/identity/index.d.ts"
    }
  },
  "scripts": {
    "build": "pnpm build-esm && build-utils pack-v2",
    "build-esm": "tsc -b tsconfig.build.json",
    "test": "vitest run --typecheck",
    "ts:check": "tsc -b tsconfig.json",
    "lint": "pnpm biome lint src/*",
    "lint:fix": "pnpm biome lint --write src/*",
    "format": "pnpm biome format src/*",
    "format:fix": "pnpm biome format --write src/*",
    "check": "pnpm biome check src/*",
    "check:fix": "pnpm biome check --write src/*"
  },
  "peerDependencies": {
    "@automerge/automerge": "^2",
    "@automerge/automerge-repo": "^2",
    "@automerge/automerge-repo-react-hooks": "^2",
    "react": "^18"
  },
  "devDependencies": {
    "@automerge/automerge": "^v2.2.9-alpha.3",
    "@automerge/automerge-repo": "^2.0.0-alpha.14",
    "@automerge/automerge-repo-network-websocket": "^2.0.0-alpha.14",
    "@automerge/automerge-repo-react-hooks": "^2.0.0-alpha.14",
    "@automerge/automerge-repo-storage-indexeddb": "^2.0.0-alpha.14",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/react": "^18.3.7",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "jsdom": "^26.0.0"
  },
  "dependencies": {
    "@effect/experimental": "^0.36.2",
    "@noble/ciphers": "^1.2.0",
    "@noble/curves": "^1.8.0",
    "@noble/hashes": "^1.7.0",
    "@noble/secp256k1": "^2.2.3",
    "@xmtp/xmtp-js": "^13.0.4",
    "@xstate/store": "^2.6.2",
    "bs58check": "^4.0.0",
    "effect": "^3.12.1",
    "fast-deep-equal": "^3.1.3",
    "siwe": "^2.3.2",
    "uuid": "^11.0.4",
    "viem": "^2.22.6"
  }
}
