{
  "name": "graph-framework-monorepo",
  "private": true,
  "type": "module",
  "workspaces": ["apps/*", "packages/*"],
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@effect/build-utils": "^0.7.8",
    "escape-string-regexp": "^5.0.0",
    "glob": "^11.0.0",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "vitest": "^2.1.4"
  },
  "scripts": {
    "clean": "node scripts/clean.mjs",
    "build": "tsc -b tsconfig.build.json && pnpm --recursive --parallel run build",
