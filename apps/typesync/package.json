{
  "name": "@graphprotocol/typesync",
  "version": "0.0.0-alpha",
  "type": "module",
  "license": "MIT",
  "description": "CLI toolchain to view existing types, select, pick, extend to create schemas and generate a @graphprotocol/hypergraph schema.",
  "repository": {
    "type": "git",
    "url": "https://github.com/graphprotocol/hypergraph.git",
    "directory": "apps/typesync"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "scripts": {
    "codegen:gql": "graphql-codegen --config ./graphql.codegen.ts",
    "build:client": "vite build",
    "build": "pnpm run build:client && tsup && pnpm copy-package-json",
    "build:ts": "tsup",
    "dev": "vite build && pnpx tsx ./src/bin.ts",
    "dev:cli": "pnpx tsx ./src/bin.ts",
    "dev:client": "vite --force",
    "clean": "rimraf dist/*",
    "start": "node ./dist/bin.cjs",
    "check": "tsc -b tsconfig.json",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "copy-package-json": "tsx scripts/copy-package-json.ts"
  },
  "devDependencies": {
    "@effect/cli": "latest",
    "@effect/experimental": "latest",
    "@effect/language-service": "latest",
    "@effect/platform": "latest",
    "@effect/platform-node": "latest",
    "@effect/sql": "latest",
    "@effect/sql-sqlite-node": "latest",
    "@effect/vitest": "latest",
    "@graphql-codegen/cli": "^5.0.5",
    "@graphql-codegen/client-preset": "^4.8.0",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-operations": "^4.6.0",
    "@tanstack/router-plugin": "^1.119.0",
    "@types/node": "^22.15.3",
    "@types/react": "^19.1.1",
    "@types/react-dom": "^19.1.3",
    "@vitejs/plugin-react": "^4.4.1",
    "tsup": "^8.4.0",
    "tsx": "^4.19.4",
    "vite": "^6.3.4"
  },
  "dependencies": {
    "@graphql-typed-document-node/core": "^3.2.0",
    "@headlessui/react": "^2.2.2",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.0.1",
    "@phosphor-icons/react": "^2.1.7",
    "@radix-ui/react-tabs": "^1.1.9",
    "@tailwindcss/vite": "^4.1.5",
    "@tanstack/react-query": "^5.75.0",
    "@tanstack/react-query-devtools": "^5.75.0",
    "@tanstack/react-router": "^1.119.0",
    "@tanstack/react-router-devtools": "1.119.0",
    "better-sqlite3": "^11.9.1",
    "date-fns": "^4.1.0",
    "effect": "latest",
    "graphql": "^16.11.0",
    "graphql-request": "^7.1.2",
    "jotai": "^2.12.3",
    "open": "^10.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.56.1",
    "shiki": "^3.3.0",
    "tailwindcss": "^4.1.5"
  }
}
